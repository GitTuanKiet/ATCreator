{
  "name": "atc",
  "version": "1.0.0",
  "description": "GitTuanKiet",
  "type": "module",
"scripts": {
  "install": "bun install",
  "check": "prettier --check 'src/**/*.{ts,js,json,html,css,scss,md}'",
  "format": "prettier --write 'src/**/*.{ts,js,json,html,css,scss,md}'",
  "lint": "bun run format && bun run check",
  "clean": "rm -rf dist && mkdir dist",
  "build": "bun run clean && bun run lint && tsc",
  "start": "bun run install && bun run build && bun dist/main.js",
  "dev": "bun --watch src/main.ts",
  "test": "jest"
},
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@google/generative-ai": "^0.2.1",
    "assemblyai": "^4.2.2",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.2",
    "express": "^4.18.2",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.2.0",
    "google-auth-library": "^9.6.3",
    "google-tts-api": "^2.0.2",
    "os": "^0.1.2",
    "pexels": "^1.4.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bun": "^1.0.5",
    "@types/cli-progress": "^3.11.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^17.0.45",
    "@types/uuid": "^9.0.8",
    "@types/fluent-ffmpeg": "^2.1.24",
    "prettier": "^3.2.5",
    "typescript": "^5.4.0-dev.20240216"
  },
  "author": "GitTuanKiet",
  "license": "MIT"
}
